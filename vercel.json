{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/ads.txt",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/static/ads.txt"
    },
    {
      "src": "/robots.txt",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/static/robots.txt"
    }
  ]
}