{
  "name": "partiguiden",
  "version": "1.0.0",
  "description": "Monorepo for all Partiguiden",
  "author": "Axel Pettersson",
  "license": "ISC",
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "pnpm lint:eslint && pnpm lint:tsc",
    "lint:eslint": "eslint",
    "lint:tsc": "turbo lint:tsc",
    "knip": "knip",
    "format:write": "prettier -w '**/*.{js,ts,tsx,md,json,css}'",
    "format:check": "prettier -c '**/*.{js,ts,tsx,md,json,css}'"
  },
  "devDependencies": {
    "@eslint/compat": "1.0.3",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.3.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "eslint": "9.3.0",
    "eslint-config-next": "14.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "typescript-eslint": "7.11.0",
    "knip": "5.17.3",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.6.0",
    "turbo": "1.13.3",
    "typescript": "5.4.5"
  }
}
