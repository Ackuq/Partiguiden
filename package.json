{
  "name": "partiguiden",
  "version": "1.0.0",
  "description": "Monorepo for all Partiguiden",
  "author": "Axel Pettersson",
  "license": "ISC",
  "packageManager": "pnpm@8.7.5",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format:write": "prettier -w '**/*.{js,ts,tsx,md,json}'",
    "format:check": "prettier -c '**/*.{js,ts,tsx,md,json}'"
  },
  "devDependencies": {
    "typescript": "5.2.2"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
