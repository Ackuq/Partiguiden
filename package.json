{
  "name": "partiguiden",
  "version": "1.0.0",
  "description": "Monorepo for all Partiguiden",
  "author": "Axel Pettersson",
  "license": "ISC",
  "packageManager": "pnpm@9.2.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:all": "turbo clean",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint",
    "lint:tsc": "turbo lint:tsc",
    "knip": "knip",
    "format:write": "prettier -w '**/*.{js,mjs,ts,tsx,md,json,css}'",
    "format:check": "prettier -c '**/*.{js,mjs,ts,tsx,md,json,css}'"
  },
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@next/eslint-plugin-next": "14.2.4",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/eslint__js": "8.42.3",
    "eslint": "9.5.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-6d3110b4d9-20240531",
    "eslint-plugin-turbo": "2.0.4",
    "knip": "5.20.0",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.4",
    "turbo": "2.0.4",
    "typescript": "5.4.5",
    "typescript-eslint": "7.13.0"
  }
}
