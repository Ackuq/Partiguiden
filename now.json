{
  "version": 2,
  "alias": ["beta.partiguiden.nu"],
  "name": "partiguiden-app",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "^/ads.txt",
      "dest": "/static/ads.txt",
      "headers": { "cache-control": "s-maxage=31536000,immutable" }
    },
    {
      "src": "^/robots.txt",
      "dest": "/static/robots.txt",
      "headers": { "cache-control": "s-maxage=31536000,immutable" }
    },
    {
      "src": "^/dokument/(?<id>[^/]+)",
      "dest": "/dokument?id=$id"
    },
    {
      "src": "^/beslut/(?<id>[^/]+)",
      "dest": "/beslut?id=$id"
    },
    {
      "src": "^/subject/(?<id>[^/]+)",
      "dest": "/subject?id=$id"
    },
    {
      "src": "^/votering/(?<id>[^/]+)/(?<bet>[^/]+)",
      "dest": "/votering?id=$id&bet=$bet"
    },
    {
      "src": "^/ledamot/(?<id>[^/]+)",
      "dest": "/ledamot?id=$id"
    }
  ]
}
