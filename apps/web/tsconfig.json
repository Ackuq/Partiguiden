{
  "extends": "@partiguiden/tsconfig",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next",
      },
    ],
    "strictNullChecks": true,
    "paths": {
      "@app/*": ["./app/*"],
      "@components/*": ["./components/*"],
      "@lib/*": ["./lib/*"],
    },
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
}
